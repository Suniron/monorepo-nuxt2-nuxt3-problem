  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.20.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-presets": "*",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "prisma": "^4.1.0",
    "supertest": "^6.3.3",
    "ts-migrate": "^0.1.35",
    "typescript": "^4.9.3"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "engines": {
    "node": ">=16.0.0"
  },
