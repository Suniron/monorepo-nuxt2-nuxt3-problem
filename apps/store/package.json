{
  "name": "@xrator-operator/store",
  "version": "1.15.0",
  "scripts": {
    "import-demo": "NODE_ENV=development node import_demo.js",
    "prestart": "node ./dist/src/prepare.js",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf coverage && rm -rf dist",
    "dev": "NODE_ENV=development PORT=3002 tsup --onSuccess \"node --inspect=0.0.0.0:9228 dist/src/app.js\"",
    "dev:docker": "pnpm install --frozen-lockfile && pnpm dev",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "start": "node ./dist/src/app.js",
    "test": "NODE_ENV=test JWT_REFRESH_LIFE_MS=123456789 jest -i --passWithNoTests",
    "test:watch": "NODE_ENV=test JWT_REFRESH_LIFE_MS=123456789 jest --watch -i --passWithNoTests",
    "lint": "eslint --ext .ts . ",
    "lint:fix": "eslint --fix --ext .ts . ",
    "db:truncate": "cross-env NODE_ENV=development dotenv -c -e ../../.env -- ts-node prisma/truncateAllDatabase.ts",
    "db:seed:demo": "cross-env NODE_ENV=development dotenv -c -e ../../.env -- ts-node prisma/startSeed.ts",
    "db:resetseed": "pnpm db:truncate && pnpm db:seed:demo",
    "prisma:playground": "node prisma/playground.prisma",
    "prisma:studio": "prisma studio",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma:update-schema": "prisma db pull && prisma generate",
    "postinstall": "prisma generate",
    "seed:reset": "node prisma/resetSeeders.js",
    "dev:compute:risks": "pnpm build && node -e 'require(\"./dist/src/tasks/computeRisks.js\").computeRiskForAllCompanies()'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "body-parser": "^1.19.0",
    "celebrate": "^13.0.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.29.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "lodash": "^4.17.21",
    "loglayer": "^1.4.2",
    "otpauth": "^9.0.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pino": "^8.8.0",
    "pino-elasticsearch": "^6.3.0",
    "pino-http": "^8.3.1",
    "pino-pretty": "^9.1.1",
    "sanitize-html": "^2.8.1",
    "swagger-ui-express": "^4.3.0",
    "unixcrypt": "^1.0.13",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.0",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/passport": "^1.0.11",
    "@types/passport-local": "^1.0.35",
    "@types/sanitize-html": "^2.8.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.31.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-presets": "workspace:*",
    "prettier": "2.2.1",
    "prisma": "^4.8.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.14.2",
  "jest": {
    "preset": "jest-presets/jest/node"
  }
}
