{
  "name": "@xrator-operator/store",
  "version": "1.13.0",
  "scripts": {
    "import-demo": "NODE_ENV=development node import_demo.js",
    "prestart": "node ./out/prepare.js",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf coverage && rm -rf out",
    "dev": "babel-node src/prepare.js && nodemon",
    "dev:docker": "pnpm install --frozen-lockfile && pnpm dev",
    "build": "node ./build.js && node ./build-seeds.js",
    "start": "node ./out/app.js",
    "test": "NODE_ENV=test JWT_REFRESH_LIFE_MS=123456789 jest -i",
    "lint": "eslint --ext .js . ",
    "lint:fix": "eslint --fix --ext .js . ",
    "db:truncate": "babel-node prisma/truncateAllDatabase.js",
    "db:seed": "babel-node prisma/startSeed.js",
    "db:resetseed": "pnpm db:truncate && pnpm db:seed",
    "prisma:playground": "node prisma/playground.prisma",
    "prisma:studio": "prisma studio",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma:update-schema": "prisma db pull && prisma generate",
    "postinstall": "prisma generate",
    "seed:reset": "babel-node prisma/resetSeeders.js",
    "compute-risks": "babel-node src/tasks/ComputeRisks.js"
  },
  "imports": {
    "@/*": "./src/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
