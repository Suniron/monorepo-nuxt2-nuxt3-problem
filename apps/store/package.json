    "@babel/runtime": "^7.17.7",
    "@prisma/client": "^4.1.0",
    "body-parser": "^1.19.0",
    "celebrate": "^13.0.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.29.3",
    "dotenv": "^8.2.0",
    "esbuild": "^0.14.49",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.0.0",
    "lodash": "^4.17.21",
    "loglayer": "^1.4.2",
    "module-alias": "^2.2.2",
    "pg": "^8.5.1",
    "pino": "^8.7.0",
    "pino-elasticsearch": "^6.3.0",
    "pino-http": "^8.2.1",
    "pino-pretty": "^9.1.1",
    "sanitize-html": "^2.7.1",
    "swagger-ui-express": "^4.3.0",
    "unixcrypt": "^1.0.13",
    "yamljs": "^0.3.0"
