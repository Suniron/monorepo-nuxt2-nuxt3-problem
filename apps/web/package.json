{
  "name": "@xrator-operator/web",
  "version": "1.13.0",
  "description": "Xrator helps to manage all your company assets and audit their security vulnerabilities",
  "author": "Eli Linares",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS=--openssl-legacy-provider nuxt",
    "dev:docker": "pnpm install --frozen-lockfile && nuxt",
