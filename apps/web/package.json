    "spa": "nuxt --spa",
    "build": "NODE_OPTIONS=--openssl-legacy-provider nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "prestart": "pnpm generate",
    "lint": "eslint --ext .js,.vue . ",
    "lint:fix": "eslint --fix --ext .js,.vue . "
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "billboard.js": "^3.4.1",
