    "cytoscape-fcose": "^2.1.0",
    "eslint": "^8.29.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.3.1",
    "jest-presets": "*",
    "prettier": "^1.16.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.14.2"
