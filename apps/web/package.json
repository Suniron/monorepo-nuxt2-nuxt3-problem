    "cytoscape-context-menus": "^4.1.0",
    "cytoscape-edgehandles": "^4.0.1",
    "cytoscape-node-html-label": "^1.2.2",
    "leaflet": "^1.9.3",
    "lodash": "^4.17.21",
    "nuxt": "^2.14.12",
    "nuxt-i18n": "^6.20.0",
    "vue": "^2.6.12",
    "vue-date-fns": "^2.0.1",
    "vue-quill-editor": "^3.0.6",
    "vue-responsive-dash": "^0.5.0",
    "vue-uuid": "^2.0.2",
    "vue2-leaflet": "^2.7.1",
